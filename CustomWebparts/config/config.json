{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "ehs-handbook-listing-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ehsHandbookListing/EhsHandbookListingWebPart.js",
          "manifest": "./src/webparts/ehsHandbookListing/EhsHandbookListingWebPart.manifest.json"
        }
      ]
    },
    "ehs-handbook-add-new-page-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ehsHandbookAddNewPage/EhsHandbookAddNewPageWebPart.js",
          "manifest": "./src/webparts/ehsHandbookAddNewPage/EhsHandbookAddNewPageWebPart.manifest.json"
        }
      ]
    },
    "ehs-handbook-navigation-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ehsHandbookNavigation/EhsHandbookNavigationWebPart.js",
          "manifest": "./src/webparts/ehsHandbookNavigation/EhsHandbookNavigationWebPart.manifest.json"
        }
      ]
    },
    "ehs-handbook-page-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ehsHandbookPageDetails/EhsHandbookPageDetailsWebPart.js",
          "manifest": "./src/webparts/ehsHandbookPageDetails/EhsHandbookPageDetailsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "EhsHandbookListingWebPartStrings": "lib/webparts/ehsHandbookListing/loc/{locale}.js",
    "EhsHandbookAddNewPageWebPartStrings": "lib/webparts/ehsHandbookAddNewPage/loc/{locale}.js",
    "EhsHandbookNavigationWebPartStrings": "lib/webparts/ehsHandbookNavigation/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "EhsHandbookPageDetailsWebPartStrings": "lib/webparts/ehsHandbookPageDetails/loc/{locale}.js"
  }
}