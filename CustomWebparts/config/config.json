{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "training-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/training/TrainingWebPart.js",
          "manifest": "./src/webparts/training/TrainingWebPart.manifest.json"
        }
      ]
    },
    "assessment-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/assessment/AssessmentWebPart.js",
          "manifest": "./src/webparts/assessment/AssessmentWebPart.manifest.json"
        }
      ]
    },
    "assessment-finder-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/assessmentFinder/AssessmentFinderWebPart.js",
          "manifest": "./src/webparts/assessmentFinder/AssessmentFinderWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "TrainingWebPartStrings": "lib/webparts/training/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "AssessmentWebPartStrings": "lib/webparts/assessment/loc/{locale}.js",
    "AssessmentFinderWebPartStrings": "lib/webparts/assessmentFinder/loc/{locale}.js"
  }
}
